<!DOCTYPE databases PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                           "http://castor.exolab.org/mapping.dtd">

<mapping>
	<class name="org.cougaar.cougaarunit.vo.TestSuiteResult">
		<map-to xml="testsuite"/>
		<field name="testList" 
			   type="org.cougaar.cougaarunit.vo.TestResultSummary"
			   collection="vector">
			<bind-xml name="testcase"/>
		</field>
		<field name="name" type="java.lang.String">
			<bind-xml name="name" node="attribute"/>
		</field>
		<field name="tests" type="java.lang.String">
			<bind-xml name="tests" node="attribute"/>
		</field>
		<field name="failures" type="java.lang.String">
			<bind-xml name="failures" node="attribute"/>
		</field>
		<field name="successrate" type="java.lang.String">
			<bind-xml name="successrate" node="attribute"/>
		</field>
	</class>
	
	<class name="org.cougaar.cougaarunit.vo.TestResultSummary">
		<map-to xml="testcase"/>
		<field name="name" type="java.lang.String">
			<bind-xml name="name" node="attribute"/>
		</field>
		<field name="idList" 
			   type="org.cougaar.cougaarunit.vo.TestResultId"
			   collection="vector">
			<bind-xml name="test"/>
		</field>
		
	</class>
	
	<class name="org.cougaar.cougaarunit.vo.TestResultId">
		<map-to xml="test"/>
		<field name="id" type="java.lang.String">
			<bind-xml name="value" node="attribute"/>
		</field>
		<field name="command" type="java.lang.String">
			<bind-xml name="command" node="element"/>
		</field>
		<field name="description" type="java.lang.String">
			<bind-xml name="description" node="element"/>
		</field>
		<field name="phase" type="java.lang.String">
			<bind-xml name="phase" node="element"/>
		</field>
		<field name="result" type="java.lang.String">
			<bind-xml name="result" node="element"/>
		</field>
	</class>
</mapping>