<!DOCTYPE databases PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                           "http://castor.exolab.org/mapping.dtd">

<mapping>
	<class name="org.cougaar.cougaarunit.vo.TestResult">
		<map-to xml="TEST"/>
		<field name="idList" 
			   type="org.cougaar.cougaarunit.vo.TestResultId"
			   collection="vector">
			<bind-xml name="ID"/>
		</field>
		<field name="name" type="java.lang.String">
			<bind-xml name="name" node="attribute"/>
		</field>
	</class>
	
	<class name="org.cougaar.cougaarunit.vo.TestResultId">
		<map-to xml="ID"/>
		<field name="id" type="java.lang.String">
			<bind-xml name="value" node="attribute"/>
		</field>
		<field name="command" type="java.lang.String">
			<bind-xml name="COMMAND" node="element"/>
		</field>
		<field name="description" type="java.lang.String">
			<bind-xml name="DESCRIPTION" node="element"/>
		</field>
		<field name="phase" type="java.lang.String">
			<bind-xml name="PHASE" node="element"/>
		</field>
		<field name="result" type="java.lang.String">
			<bind-xml name="RESULT" node="element"/>
		</field>
		<field name="expectedState"
			type="org.cougaar.cougaarunit.vo.ExpectedBlackboardState">
			<bind-xml name="EXPECTED_STATE"/>
		</field>
		<field name="actualState"
			type="org.cougaar.cougaarunit.vo.ActualBlackboardState">
			<bind-xml name="ACTUAL_STATE"/>
		</field>
	</class>
	
	<class name="org.cougaar.cougaarunit.vo.ActualBlackboardState">
		<map-to xml="ACTUAL_STATE"/>
		<field name="blackboardDeltaState" type="java.lang.String">
			<bind-xml name="BLACKBOARD_DELTA_STATE" node="element"/>
		</field>
	</class>
	<class name="org.cougaar.cougaarunit.vo.ExpectedBlackboardState">
		<map-to xml="EXPECTED_STATE"/>
		<field name="blackboardDeltaState" type="java.lang.String">
			<bind-xml name="BLACKBOARD_DELTA_STATE" node="element"/>
		</field>
	</class>
</mapping>